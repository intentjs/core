{
  "name": "@intentjs/core",
  "version": "0.1.38",
  "description": "Core module for Intent",
  "repository": {
    "type": "git",
    "url": "https://github.com/intentjs/core.git"
  },
  "bugs": {
    "url": "https://github.com/intentjs/core/issues"
  },
  "homepage": "https://github.com/intentjs/core",
  "author": "Vinayak Sarawagi <vinayaksarawagi25@gmail.com>",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && npm run copy:assets",
    "format": "prettier --write \"**/*.ts\"",
    "predev": "rimraf dist && npm run copy:assets",
    "dev": "tsc -p tsconfig.json --watch",
    "format:check": "prettier --check \"lib/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint --fix",
    "lint:check": "eslint \"{lib,tests}/**/*.ts\"",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest",
    "test:dev": "jest --watch",
    "email:dev": "email dev",
    "precopy:assets": "mkdir -p dist",
    "copy:assets": "cp -rip './resources' 'dist/resources'"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/testing": "^10.3.9",
    "@stylistic/eslint-plugin-ts": "^2.6.1",
    "@types/archy": "^0.0.36",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.13",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.4",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@intentjs/hyper-express": "^0.0.4",
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@react-email/components": "^0.0.25",
    "archy": "^1.0.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-table3": "^0.6.3",
    "console.mute": "^0.3.0",
    "dotenv": "^16.4.5",
    "enquirer": "^2.4.1",
    "eta": "^3.5.0",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "knex": "^3.1.0",
    "live-directory": "^3.0.3",
    "ms": "^2.1.3",
    "objection": "^3.1.4",
    "picocolors": "^1.1.0",
    "react-email": "^3.0.1",
    "rxjs": "^7.8.1",
    "ts-morph": "^22.0.0",
    "ulid": "^2.3.0",
    "winston": "^3.13.0",
    "yargs-parser": "^21.1.1"
  },
  "peerDependencies": {
    "knex": "^3.1.0",
    "objection": "^3.1.4"
  }
}
